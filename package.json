{
  "$schema": "https://json.schemastore.org/package.json",
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@changesets/cli": "^2.29.7",
    "@turbo/gen": "^2.5.6",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.6",
    "typescript": "catalog:",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.7.0",
    "pnpm": "^10.2.1"
  },
  "name": "gentleduck",
  "packageManager": "pnpm@10.17.0",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "rolldown"
    ],
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@ffprobe-installer/linux-x64",
      "@parcel/watcher",
      "@swc/core",
      "core-js-pure",
      "esbuild",
      "protobufjs",
      "sharp"
    ],
    "overrides": {
      "vite": "catalog:"
    }
  },
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "rimraf dist .cache .turbo node_modules .next .velite",
    "dev": "turbo dev",
    "format": "biome format --write ./ && biome check --write .",
    "lint": "biome lint --write ./",
    "lint:ws": "pnpm dlx sherif@latest",
    "release": "changeset version",
    "test": "vitest"
  },
  "version": "1.0.0"
}
