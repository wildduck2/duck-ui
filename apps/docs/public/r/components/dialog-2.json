      "content": "import { Button } from '@gentleduck/registry-ui-duckui/button'\nimport {\n  Dialog,\n  DialogClose,\n  DialogContent,\n  DialogDescription,\n  DialogFooter,\n  DialogHeader,\n  DialogTitle,\n  DialogTrigger,\n} from '@gentleduck/registry-ui-duckui/dialog'\nimport { Input } from '@gentleduck/registry-ui-duckui/input'\nimport { Label } from '@gentleduck/registry-ui-duckui/label'\nimport React from 'react'\n\nexport default function DialogDemo() {\n  const [open, setOpen] = React.useState(false)\n\n  return (\n    <Dialog onOpenChange={setOpen} open={open}>\n      <form onSubmit={(e) => e.preventDefault()}>\n        <DialogTrigger asChild>\n          <Button variant={'outline'}>Open Dialog</Button>\n        </DialogTrigger>\n        <DialogContent className=\"sm:max-w-[425px]\">\n          <DialogHeader>\n            <DialogTitle>Edit profile</DialogTitle>\n            <DialogDescription>Make changes to your profile here. Click save when you&apos;re done.</DialogDescription>\n          </DialogHeader>\n          <div className=\"grid gap-4\">\n            <div className=\"grid gap-3\">\n              <Label htmlFor=\"name-1\">Name</Label>\n              <Input defaultValue=\"Pedro Duarte\" id=\"name-1\" name=\"name\" />\n            </div>\n            <div className=\"grid gap-3\">\n              <Label htmlFor=\"username-1\">Username</Label>\n              <Input defaultValue=\"@peduarte\" id=\"username-1\" name=\"username\" />\n            </div>\n          </div>\n          <DialogFooter>\n            <DialogClose asChild>\n              <Button variant={'outline'}>Cancel</Button>\n            </DialogClose>\n            <Button type=\"submit\">Save changes</Button>\n          </DialogFooter>\n        </DialogContent>\n      </form>\n    </Dialog>\n  )\n}\n",
