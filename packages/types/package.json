{
  "$schema": "https://json.schemastore.org/package.json",
  "author": "gentleduck",
  "description": "this is a package for types and utils for typescript",
  "devDependencies": {
    "@gentleduck/biome-config": "workspace:*",
    "@gentleduck/typescript-config": "workspace:*"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    ".": "./index.ts"
  },
  "keywords": [
    "type",
    "typescript",
    "typescript-utilities"
  ],
  "license": "MIT",
  "main": "index.ts",
  "name": "@gentleduck/types",
  "packageManager": "pnpm@10.10.0",
  "peelDependencies": {},
  "private": true,
  "scripts": {
    "clean": "rimraf dist .cache .turbo node_modules .next .velite",
    "clean:git": "git clean -xdf .cache .turbo node_modules dist",
    "format": "biome format --write ./",
    "lint": "biome lint --write ./",
    "start:env": "tsx ./index.ts"
  },
  "type": "module",
  "types": "index.ts",
  "version": "0.0.1"
}
