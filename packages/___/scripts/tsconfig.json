    "incremental": true,
    "isolatedModules": true,
    "outDir": "./dist",
    "paths": {
      "@/*": ["../*"]
    },
    "tsBuildInfoFile": "./.tsbuildinfo"
  },
  "exclude": ["node_modules", "build", "dist"],
  "extends": "../../../../tsconfig.json",
  "include": ["../../ui/**/*"]
}
