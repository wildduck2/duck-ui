{
  "$schema": "https://json.schemastore.org/package.json",
  "author": "wilddcuk",
  "bugs": {
    "url": "https://github.com/gentleeduck/duck-ui/issues"
  },
  "description": "motion is a behavioral component abstraction that makes the aero dynamics of the web",
  "devDependencies": {
    "@gentleduck/tsdown-config": "workspace:*",
    "@gentleduck/typescript-config": "workspace:*",
    "tsdown": "catalog:"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    "./*": "./dist/*.js",
    "./css": "./src/index.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "tsconfig.json",
    "src/index.css"
  ],
  "homepage": "https://github.com/gentleeduck/duck-ui/tree/master/packages/duck-motion#readme",
  "name": "@gentleduck/motion",
  "packageManager": "pnpm@10.17.0",
  "peerDependencies": {
    "@gentleduck/variants": "workspace:*",
    "typescript": "catalog:"
  },
  "private": false,
  "repository": {
    "directory": "packages/duck-motion",
    "type": "git",
    "url": "https://github.com/gentleduck/ui"
  },
  "scripts": {
    "build": "tsdown",
    "ci": "pnpm run lint && pnpm run format && pnpm run build",
    "format": "biome format --write ./",
    "lint": "biome lint --write ./",
    "publish": "pnpm run ci && changeset publish",
    "release": "changeset version"
  },
  "type": "module",
  "types": "./index.ts",
  "version": "0.1.10"
}
