{
  "$schema": "https://json.schemastore.org/package.json",
  "engines": {
    "node": ">=22.0.0"
  },
  "author": "wildduck (https://github.com/wildduck2)",
  "bugs": {
    "url": "https://github.com/gentleeduck/duck-ui/issues"
  },
  "description": "motion is a behavioral component abstraction that makes the aero dynamics of the web",
  "devDependencies": {
    "@gentleduck/tsdown-config": "workspace:*",
    "@gentleduck/typescript-config": "workspace:*",
    "tsdown": "catalog:"
  },
  "exports": {
    "./*": "./dist/*.js",
    "./css": "./src/index.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "tsconfig.json",
    "src/index.css"
  ],
  "homepage": "https://github.com/gentleeduck/duck-ui/tree/master/packages/duck-motion#readme",
  "keywords": [
    "gentleduck",
    "duck-ui",
    "motion",
    "animation",
    "ui"
  ],
  "license": "MIT",
  "name": "@gentleduck/motion",
  "packageManager": "pnpm@10.17.0",
  "peerDependencies": {
    "@gentleduck/variants": "workspace:*",
    "typescript": "catalog:"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/duck-motion",
    "type": "git",
    "url": "git+https://github.com/gentleeduck/duck-ui.git"
  },
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -p tsconfig.json --noEmit --pretty false --skipLibCheck"
  },
  "type": "module",
  "types": "./index.ts",
  "version": "0.1.13"
}
