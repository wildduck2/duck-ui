{
  "$schema": "https://json.schemastore.org/package.json",
  "author": "wildduck (https://github.com/wildduck2)",
  "bugs": {
    "url": "https://github.com/gentleeduck/duck-ui/issues"
  },
  "description": "libs is a behavioral component abstraction that makes the aero dynamics of the web",
  "devDependencies": {
    "@gentleduck/tsdown-config": "workspace:*",
    "@gentleduck/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "zod": "catalog:"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    "./*": "./dist/*/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "tsconfig.json"
  ],
  "homepage": "https://github.com/gentleeduck/duck-ui/tree/master/packages/duck-libs#readme",
  "keywords": [
    "gentleduck",
    "duck-ui",
    "utilities",
    "helpers",
    "typescript"
  ],
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "name": "@gentleduck/libs",
  "packageManager": "pnpm@10.17.0",
  "peerDependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/duck-libs",
    "type": "git",
    "url": "git+https://github.com/gentleeduck/duck-ui.git"
  },
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -p tsconfig.json --noEmit --pretty false --skipLibCheck"
  },
  "type": "module",
  "version": "0.1.11"
}
